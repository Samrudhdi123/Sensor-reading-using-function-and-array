#include <stdio.h>
#include <math.h>
int main() {
    int n, temp, digit, sum = 0, count = 0;
    printf("Enter number: ");
    scanf("%d", &n);
    temp = n;
    while (temp != 0) {
        temp /= 10;
        count++;
    }
    temp = n;
    while (temp != 0) {
        digit = temp % 10;
        sum += pow(digit, count);
        temp /= 10;
    }
    if (sum == n)
        printf("%d is an Armstrong Number\n", n);
    else
        printf("%d is Not an Armstrong Number\n", n);
    return 0;
}
